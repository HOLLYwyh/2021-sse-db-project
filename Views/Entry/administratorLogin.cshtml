<!DOCTYPE html>
<html lang="zh-cn">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/css/Entry/sellerLogin/sellerLogin.css" type="text/css" rel="stylesheet" />
    <link href="/css/Common/common.css" type="text/css" rel="stylesheet" />
    <link href="~/css/Common/shortcut_five.css" type="text/css" rel="stylesheet" />
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <script src="/js/Common/vue.js"></script>
    <script src="https://unpkg.com/element-ui/lib/index.js"></script>
    @await Html.PartialAsync("~/Views/Shared/base.cshtml")
    <title>管理员登录</title>
</head>
<body>

    <div id="shortcutsi">
        <shortcut></shortcut>
    </div>

    <div class="signUp">
        <div class="huicui">
            <img src="/Images/Entry/login/logo.png" alt="">
        </div>
        <div id="ariaa" class="aria">
            <div class="login_box" id="formm">

                <div class="client_login">管理员登录</div>
                <div class="forem">

                    <el-form ref="form" :model="form" label-width="80px">

                        <el-form-item label="管理账号">
                            <el-input id="ID" v-model="form.phone"></el-input>
                        </el-form-item>

                        <el-form-item label="登录密码" class="psw">
                            <el-input id="password" v-model="form.pswd" type="password"></el-input>
                        </el-form-item>

                        <el-form-item class="rem">
                            <el-checkbox label="记住密码" name="type"></el-checkbox>
                        </el-form-item>

                        <el-form-item>
                            <el-button type="primary" @@click="onSubmit">立即登录</el-button>

                        </el-form-item>
                    </el-form>

                </div>
                <div class="have_not">

                    <a href="#"><span>返回首页</span></a>
                </div>
            </div>
        </div>
        <script type="text/javascript" src="/js/Common/shortcutFive.js"></script>
        <script type="text/javascript" src="/js/Entry/administratorLogin/administratorLogin.js"></script>
</body>
</html>